<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:LaundryManagement.ViewModel"
                    xmlns:view="clr-namespace:LaundryManagement.View">

    <DataTemplate DataType="{x:Type viewmodel:HomeViewModel}">
        <view:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:PaketViewModel}">
        <view:PaketView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:AddPaketViewModel}">
        <view:AddPaketView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:PelangganViewModel}">
        <view:PelangganView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:AddPelangganViewModel}">
        <view:AddPelangganView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:TransaksiViewModal}">
        <view:TransaksiView/>
    </DataTemplate>

</ResourceDictionary>